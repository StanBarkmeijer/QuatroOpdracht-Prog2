<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="526.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="courseTitleId" prefHeight="34.0" prefWidth="203.0" text="Course title">
            <padding>
                <Insets left="20.0" />
            </padding>
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="courseNiveauId" layoutX="582.0" prefHeight="34.0" prefWidth="100.0" text="Niveau">
            <padding>
                <Insets left="20.0" />
            </padding>
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="courseDescriptionId" alignment="TOP_LEFT" layoutY="34.0" prefHeight="87.0" prefWidth="558.0" text="Description">
            <padding>
                <Insets left="20.0" />
            </padding>
        </Label>
        <Button fx:id="visitCourseID" layoutX="582.0" layoutY="78.0" mnemonicParsing="false" onMouseClicked="#visitCourse" style="-fx-background-color: linear-gradient(from 0% 0% to 100% 100%, #E76F51, #F69982);" text="Visit course" textFill="WHITE" />
     </children>
</Pane>
