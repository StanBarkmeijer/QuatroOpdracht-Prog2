                    <Font size="14.0" />
                </font>
            </Label>
            <Label fx:id="publicationDate" layoutX="550.0" layoutY="38.0" prefHeight="34.0" prefWidth="133.0" text="Publication " />
            <Button fx:id="contentViewedButton" layoutX="550.0" layoutY="138.0" mnemonicParsing="false" style="-fx-background-color: green;" text="Content bekeken" textFill="WHITE" />
            <Label fx:id="nameSpeaker" layoutX="1.0" layoutY="72.0" prefHeight="34.0" prefWidth="133.0" text="Name speaker">
                <padding>
                    <Insets left="20.0" />
                </padding>
            </Label>
            <Label fx:id="nameOrganisation" layoutX="1.0" layoutY="101.0" prefHeight="34.0" prefWidth="133.0" text="Organisation">
